gp_lck: gp_lck.o grid.o init.o rhs.o time.o
	gfortran *.o -O3 -march=native -I/mnt/storage/apps/eb/software/FFTW/3.3.10-gompi-2021b/include -L/mnt/storage/apps/eb/software/FFTW/3.3.10-gompi-2021b/lib -lfftw3_omp -lfftw3 -lm -fopenmp -L${HOME}/json-fortran/lib -ljsonfortran -I${HOME}/json-fortran/lib -L/mnt/storage/apps/eb/software/HDF5/1.12.1-gompi-2021b/lib -lhdf5_fortran -I/mnt/storage/apps/eb/software/HDF5/1.12.1-gompi-2021b/include -o gp_lck
