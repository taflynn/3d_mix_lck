gp_lck: gp_lck.o grid.o init.o rhs.o time.o
	gfortran *.o -O3 -march=native -I/nobackup/proj/mbsmsp/libs/fftw/3.3.8/include -L/usr/lib/x86_64-linux-gnu/ -lfftw3_threads -lfftw3 -lm -fopenmp -L${HOME}/json-fortran-8.3.0/lib -ljsonfortran -I${HOME}/json-fortran-8.3.0/lib -L/nobackup/proj/mbsmsp/libs/hdf5/1.10.5-parallel/lib -lhdf5_fortran -I/nobackup/proj/mbsmsp/libs/hdf5/1.10.5-parallel/lib -o gp_lck
grid.mod: grid.f03
	gfortran -c FFTW3.f03 grid.f03 init.f03 rhs.f03 time.f03 gp_lck.f03 -I/nobackup/proj/mbsmsp/libs/fftw/3.3.8/include -L/usr/lib/x86_64-linux-gnu/ -lfftw3_threads -lfftw3 -lm -fopenmp -L${HOME}/json-fortran-8.3.0/lib -ljsonfortran -I${HOME}/json-fortran-8.3.0/lib -L/nobackup/proj/mbsmsp/libs/hdf5/1.10.5-parallel/lib -lhdf5_fortran -I/nobackup/proj/mbsmsp/libs/hdf5/1.10.5-parallel/lib
